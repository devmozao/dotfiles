[user]
  name = Diogo \"Moz√£o\" Fonseca
  email = devmozao@outlook.com
	signingkey =
[core]
  autocrlf = input
[pull]
  rebase = true
[merge]
  tool = vscode
[mergetool "vscode"]
  cmd = code --wait $MERGED
[diff]
  tool = vscode
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
[credential]
  helper = cache --timeout=28800000
[gpg]
	format = ssh
[commit]
	gpgsign = true
[diff "lockb"]
	textconv = bun
	binary = true
[init]
	defaultBranch = main
[advice]
	ignoredHook = false
